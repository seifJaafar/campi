<% if (success && success.length) {%>
    <div class="alert alert-success alert-dismissible fade show position-fixed bottom-0 end-0 z-3" " role=" alert">
        <strong><%- success %></strong>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>
        <% if(error && error.length) {%>
            <div class="alert alert-danger alert-dismissible fade show position-fixed bottom-0 end-0 z-3" " role="
                alert">
                <strong><%- error %></strong>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            <% } %>